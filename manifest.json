{
    "manifest_version": 2, 

    "name": "DataGrab",
    "description": "allows the user to copy data from a page with js selectors and turn it into an excell paste friendly string.",
    "version": "1.0",
    "options_page": "options.html",
    
    "browser_action": {
        "default_icon": "icon_128.png",
	"default_tilte": "Test Extension"
    },

    "icons": {
	"128" : "icon_128.png"
    },

    "background": {
	"scripts": ["background.js"]
    },

    "content_scripts": [{
	"matches": ["https://www.google.com/dfa/**"],
	"js": ["content.js"]
    }],

    "permissions":[
	"tabs",
        "activeTab",
	"notifications"
    ]
    
}
